    "lint:ci": "eslint src/**/* --ext. .ts,.tsx --format junit --output-file ${CIRCLE_ARTIFACTS}/eslint/results.xml",
    "lint:fix": "eslint src/**/* --ext .ts,.tsx --fix"
  },
  "author": "paveg",
  "license": "MIT",
