    "ts-jest": "24.0.2",
    "ts-loader": "6.0.4",
    "tslint": "5.18.0",
    "typescript": "3.5.3",
    "webpack": "4.39.2",
    "webpack-cli": "3.3.7",
    "webpack-dev-server": "3.8.0"
  },
  "dependencies": {
    "@types/react": "^16.8.19",
    "@types/react-dom": "^16.8.4",
    "connected-react-router": "^6.4.0",
    "enzyme": "^3.10.0",
