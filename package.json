    "@babel/preset-react": "7.0.0",
    "@babel/preset-typescript": "7.3.3",
    "@types/enzyme": "3.10.3",
    "@types/enzyme-adapter-react-16": "1.0.5",
    "@types/history": "4.7.2",
    "@types/jest": "24.0.18",
    "@typescript-eslint/eslint-plugin": "1.13.0",
    "@typescript-eslint/eslint-plugin-tslint": "1.13.0",
